            "name": "<%= propertyTypeName %>",
            "editor": {
                "view": "~/App_Plugins/YourPluginsFolder/view.html",
                "valueType": "<%= propertyTypeValue %>"
            },
            "prevalues": {
                "fields": [
                    {
                        "label": "Number of columns",
